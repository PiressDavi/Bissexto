function eBissexto(ano) {
    
    if (ano % 4 === 0) {
        if (ano % 100 === 0) {
            if (ano % 400 === 0) {
                return "Ano Bissexto"
            } else {
                return "Não é ano Bissexto"
            }
        } else {
            return "Ano Bissexto"
        }
    } else
        return "Não é ano Bissexto"
}

eBissexto(2022);
